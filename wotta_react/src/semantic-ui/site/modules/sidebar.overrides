/*******************************
         Site Overrides
*******************************/
/* Page Context */
.pushable:not(body) {
  transform: none;
}

.pushable:not(body) > .ui.sidebar,
.pushable:not(body) > .fixed,
.pushable:not(body) > .pusher:after {
  position: fixed;
}

.ui.sidebar.visible ~ .pusher {
    width: calc(100% - 150px);
}